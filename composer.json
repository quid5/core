    "keywords": ["quidphp","quid","php-framework"],
    "homepage": "https://quidphp.com",
    "license": "MIT",
    "authors": [{
        "name": "Pierre-Philippe Emond",
        "email": "emondpph@gmail.com",
        "homepage": "https://emond.io",
        "role": "Developer"
    }],
    "require": {
        "php": ">=7.4.0",
        "quidphp/base": "5.30.*",
        "quidphp/main": "5.30.*",
        "quidphp/orm": "5.30.*",
        "quidphp/routing": "5.30.*",
        "verot/class.upload.php": "^2.0",
        "phpmailer/phpmailer": "^6.6",
        "tedivm/jshrink": "^1.4",
        "scssphp/scssphp": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Quid\\Core\\": "src/",
            "Quid\\Test\\Core\\": "test/"
        }
